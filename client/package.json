    "classnames": "^2.2.1",
    "core-js": "^1.2.6",
    "csshake": "^1.5.0",
    "history": "^1.17.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
